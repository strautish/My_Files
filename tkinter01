logs=tk.Tk()
logs.geometry("300x300+1100+100")
logs.title("Loga elementi")

teksts_1=tk.Label(logs, text='''Labie darbi!'Dari vienmÄ“r labu!''',
                  bg='green',
                  fg='white',
                  font=('Arial',10,'bold'),
                  padx=100,
                  pady=20,
                  width=13,
                  height=3,
                  anchor='n',
                  relief=tk.RAISED,
                  bd=10,
                  justify=tk.CENTER
                  )
teksts_1.pack()
                

logs.mainloop()